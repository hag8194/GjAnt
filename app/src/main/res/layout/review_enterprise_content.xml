<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/enterprise_content"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toBottomOf="@+id/order_header_content"
    android:background="@drawable/background_bottom_line"
    android:paddingBottom="32dp">

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:text="@string/enterprise_data"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="24dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Title"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:text="@string/name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/label_enterprise_name"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintLeft_toLeftOf="@+id/textView4"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/enterprise_name"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        tools:text="Name here!"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/label_enterprise_name"
        app:layout_constraintLeft_toLeftOf="@+id/label_enterprise_name"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp" />

    <TextView
        android:text="Rif"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/label_enterprise_rif"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/enterprise_name"
        app:layout_constraintLeft_toLeftOf="@+id/enterprise_name"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/enterprise_rif"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        tools:text="J-3221145662"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/label_enterprise_rif"
        app:layout_constraintLeft_toLeftOf="@+id/label_enterprise_rif"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp" />

    <TextView
        android:text="@string/address"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/enterprise_phone"
        app:layout_constraintLeft_toLeftOf="@+id/enterprise_phone" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/enterprise_address"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:text="Address here!"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/phone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView20"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/enterprise_rif"
        app:layout_constraintLeft_toLeftOf="@+id/enterprise_rif" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/enterprise_phone"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        app:layout_constraintLeft_toLeftOf="@+id/textView20"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:text="Phone here!!"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

</android.support.constraint.ConstraintLayout>