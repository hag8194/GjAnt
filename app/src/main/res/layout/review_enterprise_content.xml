<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/enterprise_content"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toBottomOf="@+id/order_header_content"
    android:background="@drawable/background_bottom_line"
    android:paddingBottom="32dp">

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:text="@string/enterprise_data"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="24dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Title"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:text="@string/type"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/label_order_type"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintLeft_toLeftOf="@+id/textView4"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption" />

    <TextView
        android:text="@string/description"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/label_order_description"
        app:layout_constraintLeft_toLeftOf="@+id/textView24"
        app:layout_constraintTop_toBottomOf="@+id/textView24"
        android:layout_marginTop="16dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:text="Description here here here here Description here here here here Description here here here here Description here here here here"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/order_description"
        app:layout_constraintTop_toBottomOf="@+id/label_order_description"
        app:layout_constraintLeft_toLeftOf="@+id/label_order_description"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="Type here!"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView24"
        app:layout_constraintTop_toBottomOf="@+id/label_order_type"
        app:layout_constraintLeft_toLeftOf="@+id/label_order_type"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

</android.support.constraint.ConstraintLayout>